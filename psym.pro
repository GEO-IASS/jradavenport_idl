pro psym
; this is a shortcut program to display the standard plot symbols used
; in plot via the keyword PSYM
;
; to run, just type:
; > psym
;

compile_opt HIDDEN


;print,'The following plot symbols are available:'
print,'<JRAD> The plot symbols are:'
print,'1: +'
print,'2: *'
print,'3: .'
print,'4: Diamond'
print,'5: Triangle'
print,'6: Square'
print,'7: x'
print,'8: USERSYM'
print,'9: Undefined'
print,'10: Histogram mode'

return
end
